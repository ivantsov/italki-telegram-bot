{
  "name": "italki-telegram-bot",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "deps:install": "npm i --prefix src",
    "deps:copy": "cp -r src/node_modules dist/node_modules",
    "clean": "rm -rf dist dist.zip",
    "transpile": "babel src -d dist --ignore node_modules",
    "build": "npm-run-all clean transpile deps:*",
    "zip": "zip -r -X dist.zip dist",
    "publish": "aws lambda update-function-code --function-name checkItalkiSchedule --zip-file fileb://dist.zip --publish",
    "deploy": "npm-run-all build zip publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivantsov/italki-telegram-bot.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivantsov/italki-telegram-bot/issues"
  },
  "homepage": "https://github.com/ivantsov/italki-telegram-bot#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.10.2"
  },
  "dependencies": {
    "aws-sdk": "^2.202.0"
  }
}
